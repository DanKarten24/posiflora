{
    "name": "posiflora/backend",
    "require": {
        "php": "^8.3",
        "slim/slim": "^4.0",
        "slim/psr7": "^1.0",
        "php-di/slim-bridge": "^3.0",
        "doctrine/dbal": "^3.0",
        "guzzlehttp/guzzle": "^7.0",
        "vlucas/phpdotenv": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
